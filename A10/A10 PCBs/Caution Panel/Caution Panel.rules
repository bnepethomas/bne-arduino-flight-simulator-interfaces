    "/SEG_C0" "/SEG_C1" "/SEG_D0" "/SEG_D1" "/SEG_E0" "/SEG_E1" "/SEG_F0" "/SEG_F1"
    "/SEG_G0" "/SEG_G1" "/SEG_DP0" "/SEG_DP1"
